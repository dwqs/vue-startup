{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "MIT",
  "scripts": {
    "prepush": "npm run ilint -q",
    "dll": "npx webpack --config webpack.dll.config.js",
    "postinstall": "npm -s run dll",
    "dev": "npx cross-env NODE_ENV=development node ./build/dev-server.js",
    "build": "npx rimraf dist && npx cross-env NODE_ENV=production npx webpack --config ./build/webpack.prod.config.js --hide-modules",
    "ilint": "npx standard src/**/*.js src/**/*.vue",
    "fix": "npx standard --fix src/**/*.js src/**/*.vue",
    "commitmsg": "npx commitlint -e"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    {{#if_eq state 'vuex'}}
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    {{/if_eq}}
    {{#if_eq state 'mobx'}}
    "mobx": "^5.1.2",
    "vue-mobx": "^2.0.0",
    {{/if_eq}}
    "async-await-error-handling": "^0.5.2",
    "axios": "^0.18.0",
    "normalize.css": "^8.0.0",
    "@babel/polyfill": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^7.1.2",
    "autoprefixer": "^9.1.5",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    {{#if_eq state 'mobx'}}
    "@babel/plugin-proposal-decorators": "^7.1.0",
    {{/if_eq}}
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "rimraf": "^2.6.2",
    "eslint": "^5.6.0",
    "eslint-plugin-vue-libs": "^3.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "mini-css-extract-plugin": "^0.4.3",
    "happypack": "^5.0.0",
    "thread-loader": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "chalk": "^2.4.1",
    "plugin-error": "^1.0.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-safe-parser": "4.0.1",
    "postcss-loader": "^3.0.0",
    "husky": "^0.14.3",
    "standard": "^12.0.1",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-md5-hash": "^0.0.6",
    "webpack-inline-manifest-plugin": "^4.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack-merge": "^4.1.4"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*.svg",
      "*.less"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "standard"
      ]
    }
  },
  "engines": {
    "node": "> 8.1.4",
    "npm": ">= 5.2.0"
  }
}
