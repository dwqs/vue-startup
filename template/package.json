{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "MIT",
  "scripts": {
    "prepush": "npm run ilint -q",
    "dll": "npx webpack --config webpack.dll.config.js",
    "postinstall": "npm -s run dll",
    "dev": "npx cross-env NODE_ENV=development node ./build/dev-server.js",
    "build": "npx rimraf dist && npm run dll && npx cross-env NODE_ENV=production npx webpack --config ./build/webpack.prod.config.js --hide-modules",
    "ilint": "npx eslint src/**/*.js src/**/*.vue",
    "fix": "npx eslint --fix src/**/*.js src/**/*.vue",
    "commitmsg": "npx commitlint -e"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    {{#if_eq state 'vuex'}}
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    {{/if_eq}}
    {{#if_eq state 'revue'}}
    "revuejs": "^1.0.1",
    {{/if_eq}}
    {{#if_eq state 'mobx'}}
    "mobx": "^4.0.1",
    "vue-mobx": "^2.0.0",
    {{/if_eq}}
    "async-await-error-handling": "^0.5.2",
    "axios": "^0.18.0",
    "normalize.css": "^8.0.0",
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-stage-2": "^6.24.1",
    {{#if_eq state 'mobx'}}
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    {{/if_eq}}
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "rimraf": "^2.6.2",
    "eslint": "^4.18.2",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.1.0",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "happypack": "^5.0.0-beta.1",
    "html-webpack-plugin": "github:webpack-contrib/html-webpack-plugin",
    "gulp-util": "^3.0.8",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^2.1.1",
    "husky": "^0.14.3",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.1",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1",
    "webpack-md5-hash": "^0.0.6",
    "progress-bar-webpack-plugin": "^1.11.0",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": "> 8.1.4",
    "npm": ">= 5.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
